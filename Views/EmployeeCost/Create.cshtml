@model EmployeeCost
<form asp-action="Create" asp-controller="EmplyeeCost" asp-antiforgery="true">
    <div>
        <label asp-for="EmployeeId"></label>
        <input type="text" asp-for="EmployeeId" />
    </div>

    <div>
        <label asp-for="Description"></label>
        <input type="number" asp-for="Description" />
    </div>
    <div>
        <label asp-for="Confirmation"></label>
        <input type="number" asp-for="Confirmation" />
    </div>
    <div>
        <label asp-for="Value"></label>
        <input type="number" asp-for="Value" />
    </div>
    <div>
        <label asp-for="Status"></label>
        <input type="number" asp-for="Status" />
    </div>
    <div>
        <input type="submit" value="Save" />
    </div>
</form>