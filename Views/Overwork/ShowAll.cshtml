@model IEnumerable<Overwork>
@{
    ViewBag.Title = "Все overworks";
}
<div class="text-center">
    <h1 class="display-4">Welcome</h1>
    <a asp-action="Create">Добавить Overwork</a>

    <table class="table">
        <tr><td>ФИО</td><td>HoursCount</td><td>Status</td></tr>
        @foreach (var e in Model)
         {
    <tr>
        <td>@e.Person.FullName</td>
        <td>@e.HoursCount</td>
        <td>@e.Status</td>
        <td>
            <a asp-action="Details" asp-controller="Overwork" asp-route-id="@e.Id">Подробнее</a> |
            <a asp-action="ChangeStatus" asp-controller="Overwork" asp-route-id="@e.Id">Изменить статус</a> |
            <a asp-action="Delete" asp-controller="Overwork" asp-route-id="@e.Id">Удалить</a>
        </td>
    </tr>
         }
    </table>
</div>